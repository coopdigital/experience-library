
.el-c-primary-section {
    margin: var(--spacing-32) 0 var(--spacing-32) 0;  
    padding: var(--spacing-32) 0 var(--spacing-64) 0;
    border-bottom: 1px solid var(--color-grey-mid-light);
}

.coop-l-page .el-c-primary-section:first-of-type {
    margin-top: 0;
    padding-top: 0;
}

.el-c-primary-section p:last-of-type,
.el-c-primary-section ul:last-of-type {
    margin-bottom: var(--spacing-32);  
}

.el-c-primary-section p:last-of-type + ul  { 
    margin-top: -1rem;  
}

