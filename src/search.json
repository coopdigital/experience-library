---
layout: null
---
[
  {% for page in site.pages %}
    {
      "title"    : "{{ page.title | escape }}",
      "description" : "{{ page.intro-text-lead }}",
      "category" : "{{ page.section }}",
      "tags"     : "{{ page.tags | join: ', ' }}",
      "url"      : "{{ page.item-url | prepend: site.baseurl | replace: '//','/' }}",
      "date"     : "{{ page.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]